{
  "name": "@dfvalero/vue-ui",
  "version": "1.0.0",
  "private": true,
  "description": "UI Package for Vue apps",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && vue-cli-service build --target lib --inline-vue --name index src/index.ts",
    "build:v2": "vue-demi-switch 2 && yarn build",
    "build:v3": "vue-demi-switch 3 && yarn build"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@vue/composition-api": "^1.2.4",
    "core-js": "^3.18.1",
    "vue-demi": "0.11.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/compiler-sfc": "3.0.0",
    "babel-loader": "8.2.2",
    "node-sass": "^4.14.1",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.2",
    "typescript": "~4.1.5",
    "vue": "3.0.7",
    "vue-loader": "16.3.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.5",
    "vue": "^2.6.0 || ^3.0.0"
  }
}
